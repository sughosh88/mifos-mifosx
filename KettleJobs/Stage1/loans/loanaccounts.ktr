    <connection>SourceDB</connection>
    <sql>SELECT 
 la.ACCOUNT_ID As 	id
, la.ACCOUNT_ID As 	account_no
, ac.GLOBAL_ACCOUNT_NUM As 	external_id
, ac.CUSTOMER_ID As 	client_id
, IF ( cr.status_id = 3 , cr.PARENT_CUSTOMER_ID , null ) As group_id
, la.PRD_OFFERING_ID As product_id
, la.FUND_ID As fund_id
, cr.loan_officer_id As 	loan_officer_id
, la.BUSINESS_ACTIVITIES_ID As 	loanpurpose_cv_id
, ac.ACCOUNT_STATE_ID As 	loan_status_id
, 3 As 	loan_type_enum
, ${CUR} As 	currency_code
, ${CUR_DIGIT} As 	currency_digits
, la.LOAN_AMOUNT As 	principal_amount
, la.LOAN_AMOUNT As 	approved_pricipal
, NULL As 	arrearstolerance_amount
, IFNULL(la.INTEREST_RATE , 0.000000 ) As nominal_interest_rate_per_period
, 3 As 	interest_period_frequency_enum  -- Per year
, IFNULL(la.INTEREST_RATE , 0.000000 )  As 	annual_nominal_interest_rate
, la.INTEREST_TYPE_ID As 	interest_method_enum
, 1 As 	interest_calculated_in_period_enum  -- same as repayment freq period
, la.NO_OF_INSTALLMENTS As 	term_frequency
, rd.RECURRENCE_ID As 	term_period_frequency_enum
, rd.RECUR_AFTER As 	repay_every
, rd.RECURRENCE_ID As 	repayment_period_frequency_enum
, la.NO_OF_INSTALLMENTS As 	number_of_repayments
, IF(la.GRACE_PERIOD_TYPE_ID = 2 OR la.GRACE_PERIOD_TYPE_ID = 3 , la.GRACE_PERIOD_DURATION , NULL  ) As 	grace_on_principal_periods
, IF(la.GRACE_PERIOD_TYPE_ID = 2 , la.GRACE_PERIOD_DURATION , NULL  ) As 	grace_on_interest_periods
, NULL As 	grace_interest_free_periods
, 1    As 	amortization_method_enum
, ac.CREATED_DATE As 	submittedon_date
, ac.CREATED_BY As 	submittedon_userid
, NULL As 	approvedon_date
, NULL As 	approvedon_userid
, la.DISBURSEMENT_DATE As 	expected_disbursedon_date
, lsfirstpayment.ACTION_DATE As 	expected_firstrepaymenton_date
, la.DISBURSEMENT_DATE As 	interest_calculated_from_date
, if (ac.ACCOUNT_STATE_ID &gt; 4 AND ac.ACCOUNT_STATE_ID &lt; 10 , la.DISBURSEMENT_DATE , NULL ) As disbursedon_date
, NULL As 	disbursedon_userid
, lslastpayment.ACTION_DATE As 	expected_maturedon_date
, lslastpayment.ACTION_DATE As 	maturedon_date
, if( ac.CLOSED_DATE IS null , if( ac.ACCOUNT_STATE_ID = 10 , if ( ac.updated_date IS NULL, ac.created_date , ac.updated_date), NULL), ac.CLOSED_DATE ) As 	closedon_date
, if( ac.CLOSED_DATE = NULL , NULL , ac.UPDATED_BY )  As closedon_userid
, lsum.ORIG_FEES As 	total_charges_due_at_disbursement_derived
, if (ac.ACCOUNT_STATE_ID &gt; 4 AND ac.ACCOUNT_STATE_ID &lt; 10 , lsum.ORIG_PRINCIPAL , 0.000000 ) As 	principal_disbursed_derived
, if (ac.ACCOUNT_STATE_ID &gt; 4 AND ac.ACCOUNT_STATE_ID &lt; 10 , lsum.PRINCIPAL_PAID , 0.000000 ) As 	principal_repaid_derived
, if (ac.ACCOUNT_STATE_ID = 7 , lsum.ORIG_PRINCIPAL - lsum.PRINCIPAL_PAID , 0.000000 ) As 	principal_writtenoff_derived
, if (ac.ACCOUNT_STATE_ID = 5 OR ac.ACCOUNT_STATE_ID = 6 OR ac.ACCOUNT_STATE_ID = 9 , lsum.ORIG_PRINCIPAL - lsum.PRINCIPAL_PAID , 0.000000 ) As 	principal_outstanding_derived
, if (ac.ACCOUNT_STATE_ID &gt; 4 AND ac.ACCOUNT_STATE_ID &lt; 10 , lsum.ORIG_INTEREST , 0.000000 ) As 	interest_charged_derived
, if (ac.ACCOUNT_STATE_ID &gt; 4 AND ac.ACCOUNT_STATE_ID &lt; 10 , lsum.INTEREST_PAID , 0.000000 )  As 	interest_repaid_derived
, 0.000000 As 	interest_waived_derived
, if ( ac.ACCOUNT_STATE_ID = 7 , lsum.ORIG_INTEREST - lsum.INTEREST_PAID ,  0.000000 ) As 	interest_writtenoff_derived
, if (ac.ACCOUNT_STATE_ID = 5 OR ac.ACCOUNT_STATE_ID = 6 OR ac.ACCOUNT_STATE_ID = 9 , lsum.ORIG_INTEREST - lsum.INTEREST_PAID , 0.000000 )  As 	interest_outstanding_derived
, if (ac.ACCOUNT_STATE_ID &gt; 4 AND ac.ACCOUNT_STATE_ID &lt; 10 , lsum.ORIG_FEES , 0.000000 ) As 	fee_charges_charged_derived
, if (ac.ACCOUNT_STATE_ID &gt; 4 AND ac.ACCOUNT_STATE_ID &lt; 10 , lsum.FEES_PAID , 0.000000 ) As 	fee_charges_repaid_derived
, 0.000000 As 	fee_charges_waived_derived
, if ( ac.ACCOUNT_STATE_ID = 7 , lsum.ORIG_FEES - lsum.FEES_PAID , 0.000000 ) As 	fee_charges_writtenoff_derived
, if (ac.ACCOUNT_STATE_ID = 5 OR ac.ACCOUNT_STATE_ID = 6 OR ac.ACCOUNT_STATE_ID = 9 , lsum.ORIG_FEES - lsum.FEES_PAID , 0.000000 )  As 	fee_charges_outstanding_derived
, 0.000000 As 	penalty_charges_charged_derived
, 0.000000 As 	penalty_charges_repaid_derived
, 0.000000 As 	penalty_charges_waived_derived
, 0.000000 As 	penalty_charges_writtenoff_derived
, 0.000000 As 	penalty_charges_outstanding_derived
, if (ac.ACCOUNT_STATE_ID &gt; 4 AND ac.ACCOUNT_STATE_ID &lt; 10 , lsum.ORIG_PRINCIPAL + lsum.ORIG_INTEREST + lsum.ORIG_FEES , 0.000000 ) As 	total_expected_repayment_derived
, if (ac.ACCOUNT_STATE_ID &gt; 4 AND ac.ACCOUNT_STATE_ID &lt; 10 , lsum.PRINCIPAL_PAID + lsum.INTEREST_PAID + lsum.FEES_PAID , 0.000000 ) As 	total_repayment_derived
, if (ac.ACCOUNT_STATE_ID &gt; 4 AND ac.ACCOUNT_STATE_ID &lt; 10 , lsum.ORIG_INTEREST + lsum.ORIG_FEES , 0.000000 ) As 	total_expected_costofloan_derived
, if (ac.ACCOUNT_STATE_ID &gt; 4 AND ac.ACCOUNT_STATE_ID &lt; 10 , lsum.INTEREST_PAID + lsum.FEES_PAID , 0.000000 ) As 	total_costofloan_derived
, 0.000000 As 	total_waived_derived
, if ( ac.ACCOUNT_STATE_ID = 7 , lsum.ORIG_PRINCIPAL + lsum.ORIG_INTEREST + lsum.ORIG_FEES - lsum.PRINCIPAL_PAID - lsum.INTEREST_PAID - lsum.FEES_PAID , 0.000000 ) As 	total_writtenoff_derived
, if (ac.ACCOUNT_STATE_ID = 5 OR ac.ACCOUNT_STATE_ID = 6 OR ac.ACCOUNT_STATE_ID = 9  , lsum.ORIG_PRINCIPAL + lsum.ORIG_INTEREST + lsum.ORIG_FEES - lsum.PRINCIPAL_PAID - lsum.INTEREST_PAID - lsum.FEES_PAID , 0.000000 ) As 	 total_outstanding_derived
, NULL As 	total_overpaid_derived
, if( ac.ACCOUNT_STATE_ID = 10 , if(ac.closed_date IS NULL, if ( ac.updated_date IS NULL, ac.created_date , ac.updated_date), ac.closed_date) , NULL ) As 	rejectedon_date
, NULL As 	rejectedon_userid
, NULL As 	rescheduledon_date
, NULL As 	withdrawnon_date
, NULL As 	withdrawnon_userid
, if( ac.ACCOUNT_STATE_ID = 7 , ac.closed_date , NULL ) As 	writtenoffon_date
, 1    As 	loan_transaction_strategy_id  -- MIfos Style
, 0    As   is_npa
FROM loan_account la
JOIN account ac ON ac.ACCOUNT_ID = la.ACCOUNT_ID
JOIN customer cr ON cr.CUSTOMER_ID = ac.CUSTOMER_ID
JOIN meeting me ON me.MEETING_ID = la.MEETING_ID
JOIN recurrence_detail rd ON rd.MEETING_ID = me.MEETING_ID
LEFT JOIN loan_schedule lsfirstpayment ON lsfirstpayment.ACCOUNT_ID = ac.ACCOUNT_ID AND lsfirstpayment.INSTALLMENT_ID = 1
LEFT JOIN loan_schedule lslastpayment ON lslastpayment.ACCOUNT_ID = ac.ACCOUNT_ID AND lslastpayment.INSTALLMENT_ID = la.NO_OF_INSTALLMENTS
LEFT JOIN loan_summary lsum ON lsum.ACCOUNT_ID = ac.ACCOUNT_ID</sql>
