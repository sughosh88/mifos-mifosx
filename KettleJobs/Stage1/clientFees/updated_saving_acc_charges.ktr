  <step>
    <name>Input Account fees</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>DestinationDB</connection>
    <sql>SELECT
cfa.fee_account_id As fee_account_id,  
 cl.default_savings_account AS savings_account_id,
 ch.id AS charge_id,
 0 AS is_penalty,
 ch.charge_time_enum AS charge_time_enum,
 NULL AS charge_due_date,  -- derived in next step
 NULL AS fee_on_month,
 NULL AS fee_on_day,  -- Need to be derived per account based o center meeting day
 ch.fee_interval AS fee_interval,
 ch.charge_calculation_enum AS charge_calculation_enum,
 cfa.amount AS amount,
 SUM(cfa.amount_paid_derived) AS amount_paid_derived,
 SUM(cfa.amount)- SUM(cfa.amount_paid_derived) AS amount_outstanding_derived,
 cfa.is_paid_derived AS is_paid_derived,
 0 AS waived,
 cfa.is_active AS is_active,
 1 As is_calendar_inherited,
&apos;2014-06-06&apos; As start_date  -- this dummy date next ktr will override it
FROM stage1_client_fees_account cfa
INNER JOIN m_client cl ON cfa.client_id=cl.id 
INNER JOIN m_charge ch ON cfa.charge_id=ch.id 
group by  cfa.fee_account_id</sql>
