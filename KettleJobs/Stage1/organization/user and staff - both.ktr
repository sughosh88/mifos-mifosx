    <sql>SELECT DISTINCT
  pl.PERSONNEL_ID
, pl.LEVEL_ID
, pl.GLOBAL_PERSONNEL_NUM
, pl.OFFICE_ID
, pl.TITLE
, pl.PERSONNEL_STATUS
, pl.PASSWORD
, pl.LOGIN_NAME
, pl.EMAIL_ID
, CONCAT(pl.DISPLAY_NAME , IF(dupNames.PERSONNEL_ID IS NOT NULL , pl.PERSONNEL_ID , &apos;&apos;)) AS DISPLAY_NAME
, pl.CREATED_BY
, pl.CREATED_DATE
, pl.LAST_LOGIN
, pl.LOCKED
, pd.FIRST_NAME
, pd.MIDDLE_NAME
, pd.LAST_NAME
, CURDATE()As last_time_password_updated
FROM personnel pl
LEFT JOIN personnel_details pd ON pd.PERSONNEL_ID = pl.PERSONNEL_ID
LEFT JOIN personnel dupNames ON dupNames.DISPLAY_NAME = pl.DISPLAY_NAME AND dupNames.PERSONNEL_ID &lt;&gt; pl.PERSONNEL_ID  
JOIN office of ON of.OFFICE_ID = pl.OFFICE_ID</sql>
